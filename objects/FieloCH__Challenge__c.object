    <validationRules>
        <fullName>FieloCH__Active_Challenge</fullName>
        <active>true</active>
        <errorConditionFormula>FieloCH__IsActive__c &amp;&amp;  (FieloCH__MissionsCounter__c == 0 || FieloCH__RewardCounter__c == 0)</errorConditionFormula>
        <errorMessage>Can&apos;t Activate a Challenge without a mission or reward</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloCH__Competition_Type_Required</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( FieloCH__CompetitionType__c , &quot;&quot;)</errorConditionFormula>
        <errorDisplayField>FieloCH__CompetitionType__c</errorDisplayField>
        <errorMessage>Competition Type is Required when Mode is selected</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloCH__Edit_Challenge_Competition</fullName>
        <active>true</active>
        <description>If the competition type is modified, check that is has no missions associated.</description>
        <errorConditionFormula>IF( AND(FieloCH__MissionsCounter__c &gt; 0,   ISCHANGED(FieloCH__CompetitionType__c) ), true, false)</errorConditionFormula>
        <errorDisplayField>FieloCH__CompetitionType__c</errorDisplayField>
        <errorMessage>Cannot change Competition Type if already have missions.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloCH__Edit_Challenge_Mode</fullName>
        <active>true</active>
        <description>Check if it has missions or members associated in order to let the user modify the challenge.</description>
        <errorConditionFormula>IF( AND( FieloCH__MissionsCounter__c &gt; 0, ISCHANGED( FieloCH__Mode__c ) ), true, false)</errorConditionFormula>
        <errorDisplayField>FieloCH__Mode__c</errorDisplayField>
        <errorMessage>Cannot change Mode if already have missions or community associated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloCH__Edit_Competition_Type</fullName>
        <active>true</active>
        <description>Check if it has rewards associated.</description>
        <errorConditionFormula>AND(If( FieloCH__RewardCounter__c &gt; 0, true, false ) , ISCHANGED(FieloCH__CompetitionType__c))</errorConditionFormula>
        <errorDisplayField>FieloCH__CompetitionType__c</errorDisplayField>
        <errorMessage>Cannot modified Competition Type if you had associated rewards. Go to &quot;Manage Rewards&quot;  and delete them first.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloCH__Final_Date_After_Initial_Date</fullName>
        <active>true</active>
        <errorConditionFormula>FieloCH__InitialDate__c &gt;  FieloCH__FinalDate__c</errorConditionFormula>
        <errorMessage>Initial Date must be before Final Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloCH__Mode_Required</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(FieloCH__Mode__c, &apos;&apos;)</errorConditionFormula>
        <errorDisplayField>FieloCH__Mode__c</errorDisplayField>
        <errorMessage>Mode must be selected</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloCH__Program_Required</fullName>
        <active>true</active>
        <description>Checks that the challenge has a program when is edited.</description>
        <errorConditionFormula>AND(NOT( ISNEW() ), OR(ISNULL( FieloCH__Program__c ), ISBLANK(FieloCH__Program__c) ) )</errorConditionFormula>
        <errorDisplayField>FieloCH__Program__c</errorDisplayField>
        <errorMessage>The Challenge must belong to a Program.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloCH__Recurrence_Type_Valid</fullName>
        <active>true</active>
        <description>Check that the Recurrence Type choosen is valid for the Mode and for the Competition Type</description>
        <errorConditionFormula>AND(
    OR( ISPICKVAL( FieloCH__CompetitionType__c , &quot;Performance&quot;), ISPICKVAL(  FieloCH__CompetitionType__c , &quot;Time&quot;)),
    FieloCH__Repeatable__c = TRUE
)</errorConditionFormula>
        <errorMessage>Repeatable is valid only for Objective Competition Type</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloCH__Subscription_Mode_Required</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(FieloCH__Subscription__c, &quot;&quot;)</errorConditionFormula>
        <errorDisplayField>FieloCH__Subscription__c</errorDisplayField>
        <errorMessage>Subscription Mode is Required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloCH__Subscription_Period</fullName>
        <active>true</active>
        <description>Check that the period define for subscription is not greater than the duration of the Challenge</description>
        <errorConditionFormula>FieloCH__SubscribeTo__c &gt; FieloCH__FinalDate__c</errorConditionFormula>
        <errorDisplayField>FieloCH__SubscribeTo__c</errorDisplayField>
        <errorMessage>Subscription period Final Date cannot be greater than Challenge Final Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloCH__Subscription_Period_Valid_Date</fullName>
        <active>true</active>
        <description>Check that the period is the same or before from the Active period of the Challenge.</description>
        <errorConditionFormula>AND( NOT( OR( ISBLANK(FieloCH__SubscribeFrom__c), ISNULL(FieloCH__SubscribeFrom__c) )),
OR( FieloCH__InitialDate__c &lt;  FieloCH__SubscribeFrom__c, FieloCH__SubscribeFrom__c &gt;  FieloCH__SubscribeTo__c ))</errorConditionFormula>
        <errorDisplayField>FieloCH__SubscribeFrom__c</errorDisplayField>
        <errorMessage>The Subscription Initial Date cannot be greater than Subscription Final Date or greater than Challenge Initial Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloCH__Visibility_False_for_Subscription_Close</fullName>
        <active>true</active>
        <description>When the subscription is closed, the visibility must be false so it wont be shown.</description>
        <errorConditionFormula>AND( ISPICKVAL( FieloCH__Subscription__c  , &apos;Open&apos;),  NOT(FieloCH__Visible__c)  )</errorConditionFormula>
        <errorDisplayField>FieloCH__Visible__c</errorDisplayField>
        <errorMessage>If Subscription Mode is Open then visibility must be checked.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
    <webLinks>
        <fullName>FieloCH__Active</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Change Status</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT (&quot;/soap/ajax/23.0/connection.js&quot;)}

var missionsChallenge = sforce.connection.query(&quot;SELECT Id, Name, {!$Setup.FieloEE__PublicSettings__c.FieloEE__ChallengesPrefix__c}Mission__r.RecordType.DeveloperName FROM {!$Setup.FieloEE__PublicSettings__c.FieloEE__ChallengesPrefix__c}MissionChallenge__c WHERE {!$Setup.FieloEE__PublicSettings__c.FieloEE__ChallengesPrefix__c}Challenge__c = \&apos;{!FieloCH__Challenge__c.Id}\&apos; &quot;);

var records = missionsChallenge.getArray(&quot;records&quot;);
var withObjective = new Array();
var withoutObjective = new Array();

for (var i = 0; i &lt; records.length; i++) {
    var record = records[i];
    if(record.{!$Setup.FieloEE__PublicSettings__c.FieloEE__ChallengesPrefix__c}Mission__r.RecordType.DeveloperName == &apos;WithObjective&apos;){
        withObjective.push(record);
    }else{
        withoutObjective.push(record);
    }
}

if(&quot;{!FieloCH__Challenge__c.FieloCH__CompetitionType__c}&quot; == &quot;Performance&quot; &amp;&amp; withoutObjective == &apos;&apos;){
    alert(&apos;{!$Label.fieloch__cantactivateperformancewithoutobjective}&apos;);
}else{
    //create challenge and update list
    var newChallenge = new sforce.SObject(&quot;{!$Setup.FieloEE__PublicSettings__c.FieloEE__ChallengesPrefix__c}Challenge__c&quot;);
    var challengesToUpdate = [];

    //set challenge with current challenge&apos;s ID
    newChallenge.Id = &apos;{!FieloCH__Challenge__c.Id}&apos;;

    // logically invert &apos;isActive&apos;
    newChallenge.{!$Setup.FieloEE__PublicSettings__c.FieloEE__ChallengesPrefix__c}IsActive__c = ! {!FieloCH__Challenge__c.FieloCH__IsActive__c};

    //adds challenge to update to update list
    challengesToUpdate.push(newChallenge);

    //updates list
    result = sforce.connection.update(challengesToUpdate);

    if ( !result[0].getBoolean(&quot;success&quot;) ) {
        alert(result[0].getArray(&quot;errors&quot;)[0].message );
    } else {
        if(withObjective == &apos;&apos; &amp;&amp; {!FieloCH__Challenge__c.FieloCH__IsActive__c} == 0){
            alert(&apos;There are no missions with objective, so there is the chance that some members could win a reward without making any action.&apos;);
        }
        //refreshes window
        window.location.reload();
    }
}</url>
    </webLinks>
