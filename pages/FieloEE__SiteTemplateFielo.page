<apex:page applyHtmlTag="false" applyBodyTag="false" showHeader="false" id="SiteTemplate" standardStylesheets="false" language="{!language}" controller="FieloEE.SiteTemplateFieloController" cache="true" action="{!redirect}" >
<apex:variable var="page" value="{!IF(ISNULL($CurrentPage.parameters.idMenu),IF($CurrentPage.Name == 'Page','Home',$CurrentPage.Name),'PageID_'+$CurrentPage.parameters.idMenu)}"/>

<html class="layout--is{!layoutId} menu--is{!page} browser--is{!browser} os--is{!os}">
  <head>
    <title><c:Head content="seo-title" /> | {!layout.FieloEE__Title__c}</title>
    <c:Head content="seo-keywords" />
    <c:Head content="seo-description" />
    <apex:insert name="FieloStylesheets">
        <script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
        <script src="//maxcdn.bootstrapcdn.com/bootstrap/2.3.0/js/bootstrap.min.js"></script>
        <script src="{!URLFOR($Resource.js, 'core.min.js')}" type="text/javascript"></script>
        <link href="//maxcdn.bootstrapcdn.com/bootstrap/2.3.1/css/bootstrap.min.css" rel="stylesheet"></link>
        <link href="//maxcdn.bootstrapcdn.com/bootstrap/2.3.1/css/bootstrap-responsive.min.css" rel="stylesheet"></link>
        <link href="//maxcdn.bootstrapcdn.com/font-awesome/3.2.1/css/font-awesome.min.css" rel="stylesheet"></link>

        <!-- Bootstrap -->
            <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1"/>
        <!-- Fin Bootstrap -->
        
        <!-- Font Awesome -->
            <!--[if IE 7]>
                <apex:stylesheet value="{!URLFOR($Resource.css, 'font-awesome-ie7.css')}"/>
            <![endif]-->
        <!-- Fin Font Awesome -->
        <c:CSS />
    </apex:insert>
    <c:Head content="advanced-html-general" />
    <c:Head content="advanced-html-layout" />
    <c:Head content="page-css" />
  </head>    
  <body>
    
    <apex:variable var="headClass" value="fullsize"/>
    <apex:variable var="headClass" value="container" rendered="{!layout.FieloEE__Header__c == 'pagesize'}"/>
    <apex:variable var="headClass" value="fixed" rendered="{!layout.FieloEE__Header__c == 'fixed'}"/>
    
    <apex:variable var="footClass" value="fullsize"/>
    <apex:variable var="footClass" value="container" rendered="{!layout.FieloEE__Footer__c == 'pagesize'}"/>
    <apex:variable var="footClass" value="fixed" rendered="{!layout.FieloEE__Footer__c == 'fixed'}"/>
    
    <apex:insert name="top"/>
    
    <apex:insert name="head">
        <header class="{!headClass}">
            <c:SiteHeaderFielo />
        </header>
    </apex:insert>
    
    <div class="container bodyContainer head{!headClass} foot{!footClass}">
        <div class="container-fluid">
            <div class="row-fluid">
                <apex:pageMessages />
                <apex:insert name="body" />
            </div>
        </div>
    </div>
    
    <apex:insert name="foot">
        <footer class="{!footClass}">
            <div class="container">
                <c:SiteFooterFielo />
            </div>
        </footer>
    </apex:insert>
    
    <apex:insert name="bottom"/>
    

    <site:previewAsAdmin />
        
    <apex:insert name="analytics">
        <site:googleAnalyticsTracking />    
        <apex:panelGroup layout="none" rendered="{!(!ISNULL($Setup.FieloEE__PublicSettings__c.FieloEE__NewsView__c) && !ISBLANK($Setup.FieloEE__PublicSettings__c.FieloEE__NewsView__c))}" >
            <script>
              (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
              (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
              m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
              })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
            
              ga('create', '{!$Setup.FieloEE__PublicSettings__c.FieloEE__NewsView__c}', 'auto');
              ga('send', 'pageview');
            
            </script>
        </apex:panelGroup>
    </apex:insert>
  </body>
</html>
</apex:page>