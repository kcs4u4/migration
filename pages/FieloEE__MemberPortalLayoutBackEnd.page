            </apex:panelGroup>
            {!headAtt};
            {!backAtt};
        }
        #pr-head 
        {
            height: 60px;
            <apex:panelGroup layout="none" rendered="{!NOT(ISNULL(FieloEE__ProgramLayout__c.FieloEE__headerBackgroundId__c))}">
                background-image: url({!IF(LEFT(FieloEE__ProgramLayout__c.FieloEE__headerBackgroundId__c,3) == '00P', URLFOR($Action.Attachment.Download, FieloEE__ProgramLayout__c.FieloEE__headerBackgroundId__c), IF(LEFT(FieloEE__ProgramLayout__c.FieloEE__headerBackgroundId__c,3) == '015', URLFOR($Action.Document.Download, FieloEE__ProgramLayout__c.FieloEE__headerBackgroundId__c), URLFOR($Resource.FieloEE__Templates, FieloEE__ProgramLayout__c.FieloEE__headerBackgroundId__c)))});
            </apex:panelGroup>
            {!headAtt};
        }
        #pr-body
        {
            width: 75%;
            margin: 0 auto;
            <apex:panelGroup layout="none" rendered="{!NOT(ISNULL(FieloEE__ProgramLayout__c.FieloEE__BodyBackgroundId__c))}">
            background-image:url({!IF(LEFT(FieloEE__ProgramLayout__c.FieloEE__BodyBackgroundId__c,3) == '00P', URLFOR($Action.Attachment.Download, FieloEE__ProgramLayout__c.FieloEE__BodyBackgroundId__c), IF(LEFT(FieloEE__ProgramLayout__c.FieloEE__BodyBackgroundId__c,3) == '015', URLFOR($Action.Document.Download, FieloEE__ProgramLayout__c.FieloEE__BodyBackgroundId__c), URLFOR($Resource.FieloEE__Templates, FieloEE__ProgramLayout__c.FieloEE__BodyBackgroundId__c)))});
            </apex:panelGroup>
            {!bodyAtt};
        }
        #pr-foot
        {
            height: 50px;
            <apex:panelGroup layout="none" rendered="{!NOT(ISNULL(FieloEE__ProgramLayout__c.FieloEE__FooterBackgroundId__c))}">
            background-image: url({!IF(LEFT(FieloEE__ProgramLayout__c.FieloEE__FooterBackgroundId__c,3) == '00P', URLFOR($Action.Attachment.Download, FieloEE__ProgramLayout__c.FieloEE__FooterBackgroundId__c), IF(LEFT(FieloEE__ProgramLayout__c.FieloEE__FooterBackgroundId__c,3) == '015', URLFOR($Action.Document.Download, FieloEE__ProgramLayout__c.FieloEE__FooterBackgroundId__c), URLFOR($Resource.FieloEE__Templates, FieloEE__ProgramLayout__c.FieloEE__FooterBackgroundId__c)))});
            </apex:panelGroup>
            {!footAtt};
        }

        .imgLogo        { height:50px; margin:5px; }
        .imgLogoFoot    { height:30px; margin:5px 20px; }

        #pr-head.fixed          { width:100%; position:absolute; top: 0; }
        #pr-head.fullsize       { width:100%; }
        #pr-head.pagesize       { width: 75%; margin: 0 auto; }
        #pr-head.fixed .innerhead, 
        #pr-head.fullsize .innerhead    { width:75%; margin: 0 auto; }
        #pr-head.pagesize .innerhead    { width:100%; }

        #pr-foot.fixed          { width:100%; position:absolute; bottom: 0; }
        #pr-foot.fullsize       { width:100%; }
        #pr-foot.pagesize       { width: 75%; margin: 0 auto; }
        #pr-foot.fixed .innerfoot, 
        #pr-foot.fullsize .innerfoot    { width:75%; margin: 0 auto; }
        #pr-foot.pagesize .innerfoot    { width:100%; }

        #pr-body.fixed  { padding-top: 60px; }

        #pr-menu li     { float:left; list-style:none; }
        #pr-menu .amenu { float:left; list-style:none; color: #{!FieloEE__ProgramLayout__c.FieloEE__Color05__c}; }
        #menu-active        { color:#{!FieloEE__ProgramLayout__c.FieloEE__ColorFont06__c}; }
        #pr-menu.tabs li    { border-radius: 4px 4px 0 0; padding:1px 6px; }
        #pr-menu.pills li   { border-radius: 4px; padding:1px 6px; margin-bottom:3px; }
