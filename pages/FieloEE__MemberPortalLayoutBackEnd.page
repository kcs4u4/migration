    <script>
        
        window.onload = function doTransparent()
        {
            if ( '{!FieloEE__ProgramLayout__c.FieloEE__Color02__c}' == '' )   {   document.getElementById('noColor2').checked = true;     document.getElementById('page:form:pageBlock:color2').value = ' ';  }else{  document.getElementById('yesColor2').checked = true;    }
            if ( '{!FieloEE__ProgramLayout__c.FieloEE__Color03__c}' == '' )   {   document.getElementById('noColor3').checked = true;     document.getElementById('page:form:pageBlock:color3').value = ' ';  }else{  document.getElementById('yesColor3').checked = true;    }
            if ( '{!FieloEE__ProgramLayout__c.FieloEE__Color04__c}' == '' )   {   document.getElementById('noColor4').checked = true;     document.getElementById('page:form:pageBlock:color4').value = ' ';  }else{  document.getElementById('yesColor4').checked = true;    }
            if ( '{!FieloEE__ProgramLayout__c.FieloEE__Color06__c}' == '' )   {   document.getElementById('noColor6').checked = true;     document.getElementById('page:form:pageBlock:color6').value = ' ';  }else{  document.getElementById('yesColor6').checked = true;    }
            if ( '{!FieloEE__ProgramLayout__c.FieloEE__Color07__c}' == '' )   {   document.getElementById('noColor7').checked = true;     document.getElementById('page:form:pageBlock:color7').value = ' ';  }else{  document.getElementById('yesColor7').checked = true;    }
            if ( '{!FieloEE__ProgramLayout__c.FieloEE__Color08__c}' == '' )   {   document.getElementById('noColor8').checked = true;     document.getElementById('page:form:pageBlock:color8').value = ' ';  }else{  document.getElementById('yesColor8').checked = true;    }
            if ( '{!FieloEE__ProgramLayout__c.FieloEE__Color09__c}' == '' )   {   document.getElementById('noColor9').checked = true;     document.getElementById('page:form:pageBlock:color9').value = ' ';  }else{  document.getElementById('yesColor9').checked = true;    }
            if ( '{!FieloEE__ProgramLayout__c.FieloEE__Color10__c}' == '' )   {   document.getElementById('noColor10').checked = true;    document.getElementById('page:form:pageBlock:color10').value = ' '; }else{  document.getElementById('yesColor10').checked = true;   }
            if ( '{!FieloEE__ProgramLayout__c.FieloEE__Color11__c}' == '' )   {   document.getElementById('noColor11').checked = true;    document.getElementById('page:form:pageBlock:color11').value = ' '; }else{  document.getElementById('yesColor11').checked = true;   }
        }

        function changeFont(font)
        {
            var myObj  = document.getElementById('fontPreview');
            var myObj2 = document.getElementById('fontPreviewText');
            var body   = document.getElementById('pr-back');

            if(font.value == null){
                myObj.innerHTML = font;
                myObj.style.fontFamily = font;
                myObj2.style.fontFamily = font;
                body.style.fontFamily = font;
            }else{
                myObj.innerHTML = font.value;
                myObj.style.fontFamily = font.value;
                myObj2.style.fontFamily = font.value;
                body.style.fontFamily = font.value;
            }
        }

        function changeMenuColor (input,color) {
            var fields = document.getElementsByClassName('amenu');
            for (var i = 0; i < fields.length; i++) {
                fields[i].style.color='#'+color.value;
            }
            document.getElementById('page:form:pageBlock:color'+input).value = color.value;
        }

        function changeImage(type)
        {
            if (document.getElementById(type+'Preview').style.display == 'none'){
                document.getElementById(type+'Preview').style.display = 'block';
            }else{
                document.getElementById(type+'Preview').style.display = 'none';
            }
            if (document.getElementById(type+'Upload').style.display == 'none'){
                document.getElementById(type+'Upload').style.display = 'block';
            }else{
                document.getElementById(type+'Upload').style.display = 'none';
            }
            if ( type != 'logo' ){
                if (document.getElementById(type+'Attributes').style.display == 'none'){
                    document.getElementById(type+'Attributes').style.display = 'block';
                }else{
                    document.getElementById(type+'Attributes').style.display = 'none';
                }
            }
        }

        function changeAtributte(divid,att)
        {
            if ( divid == 'pr-back' ){
                if ( att.value == '100%' ){
                    document.getElementsByClassName('backgroundImage')[0].style.backgroundSize = att.value;
                    document.getElementsByClassName('backgroundImage')[0].style.backgroundRepeat = 'No-repeat';
                }else{
                    document.getElementsByClassName('backgroundImage')[0].style.backgroundRepeat = att.value;
                    document.getElementsByClassName('backgroundImage')[0].style.backgroundSize = 'auto auto';
                }
            }else{
                if ( att.value == '100%' ){
                    document.getElementById(divid).style.backgroundSize = att.value;
                    document.getElementById(divid).style.backgroundRepeat = 'No-repeat';
                }else{
                    document.getElementById(divid).style.backgroundRepeat = att.value;
                    document.getElementById(divid).style.backgroundSize = 'auto auto';
                }
            }
        }

        function changeColor(input,divid,color)
        {
            if (input == 2){
                if ( document.getElementById('imgLogo').src.indexOf('fielo-') > 0 ){
                    {!$RemoteAction.MemberPortalLayoutBackEndController.getColor}(color.value,function(result,event)
                    {
                        document.getElementById('imgLogo').src = document.getElementById('imgLogo').src.replace("000000",result);
                        document.getElementById('imgLogo').src = document.getElementById('imgLogo').src.replace("FFFFFF",result);
                    }, {escape:true});
                }
            }
            if (input == 4){
                {!$RemoteAction.MemberPortalLayoutBackEndController.getColor}(color.value,function(result,event)
                {
                    document.getElementById('imgLogoFoot').src = document.getElementById('imgLogoFoot').src.replace("000000",result);
                    document.getElementById('imgLogoFoot').src = document.getElementById('imgLogoFoot').src.replace("FFFFFF",result);
                }, {escape:true});
            }
            if (input == 1){
                    if ( color.value == ' ' ) {
                        document.getElementById('page:form:pageBlock:color'+input).value = ' ';
                    }else{
                        if ( color.color != null) {
                            document.getElementById(divid).style.backgroundColor = '#'+color.color;
                        }else{
                            document.getElementById(divid).style.backgroundColor = '#'+color.value;
                        }
                        document.getElementById('page:form:pageBlock:color'+input).value = color.value;
                    }
            }else{
                if ( color.value == ' '){
                    document.getElementById('yesColor'+input).checked = false;
                    document.getElementById('yesColor'+input).value = document.getElementById('page:form:pageBlock:color'+input).value;
                    document.getElementById(divid).style.backgroundColor = null;
                    document.getElementById('page:form:pageBlock:color'+input).value = ' ';
                }else{
                    if ( color.color != null){
                        document.getElementById(divid).style.backgroundColor = '#'+color.color;
                    }else{
                        document.getElementById(divid).style.backgroundColor = '#'+color.value;
                    }
                    document.getElementById('noColor'+input).checked = false;
                    document.getElementById('yesColor'+input).checked = true;
                    document.getElementById('page:form:pageBlock:color'+input).value = color.value;
                }
            }
        }

        function changeBadgeColor(input,color)
        {
            var fields = document.getElementsByClassName('badge');
            if ( color.value == ' '){
                document.getElementById('yesColor'+input).checked = false;
                document.getElementById('yesColor'+input).value = document.getElementById('page:form:pageBlock:color'+input).value;
                for (var i = 0; i < fields.length; i++) {
                    fields[i].style.backgroundColor='Transparent';
                }
                document.getElementById('page:form:pageBlock:color'+input).value = ' ';
            }else{
                if ( color.color != null){
                    for (var i = 0; i < fields.length; i++) {
                        fields[i].style.backgroundColor= '#'+color.color;
                    }
                }else{
                    for (var i = 0; i < fields.length; i++) {
                        fields[i].style.backgroundColor= '#'+color.value;
                    }
                }
                document.getElementById('noColor'+input).checked = false;
                document.getElementById('yesColor'+input).checked = true;
                document.getElementById('page:form:pageBlock:color'+input).value = color.value;
            }
        }

        function getOppositeColor(target,color,input)
        {
            {!$RemoteAction.MemberPortalLayoutBackEndController.getColor}(color.value,function(result,event)
            {
                var fields = document.getElementsByClassName(target);
                for (var i = 0; i < fields.length; i++) {
                    fields[i].style.color='#'+result;
                }
                document.getElementById('page:form:pageBlock:colorFont'+input).value = result;
            }, {escape:true});
        }

        function setFocusOnLoad() {}

    </script> 
    
    <apex:form id="form" styleClass="form-horizontal">
            <apex:sectionHeader title="{!$Label.fieloee__titlememberportalsettings}"/>
            <apex:pagemessages />

            <apex:pageblock title="{!$Label.fieloee__layoutconfiguration}" helpUrl="FieloHelp?hlp=CustomizeSite" helpTitle="" id="pageBlock">
                <apex:pageBlockButtons >
                    <apex:commandButton action="{!doSave}" value="{!$Label.fieloee__save}"/>  
                    <apex:commandButton action="{!doQuickSave}" value="{!$Label.fieloee__quicksave}"/>                 
                    <apex:commandButton action="{!cancel}" value="{!$Label.fieloee__cancel}"/>
                </apex:pageBlockButtons>

                <apex:pageBlockSection columns="1">

                    <!-- Segment -->
                    <apex:panelGroup >
                        <div class="control-group">
                            <apex:outputLabel styleClass="control-label" >{!$ObjectType.FieloEE__ProgramLayout__c.fields['Segment__c'].Label}</apex:outputLabel>
                            <div class="controls">
                                <apex:panelGroup rendered="{!ISNULL(FieloEE__ProgramLayout__c.Id)}" >
                                    <apex:selectList value="{!segmentSelected}" size="1" >
                                        <apex:selectOptions value="{!segmentsOptions}" />
                                    </apex:selectList>
                                </apex:panelGroup>
                                <apex:panelGroup rendered="{!!ISNULL(FieloEE__ProgramLayout__c.Id)}" >
                                    <apex:outputText value="{!segmentSelected}" style="color:#BBBBBB; font-size:25px; font-weight:bold;" />
                                </apex:panelGroup>
<!--                             <span class="help-inline">{!$Label.InfoLayoutTitle}</span> -->
                            </div>
                        </div>
                    </apex:panelGroup>

                    <!-- Select template -->
                    <apex:panelGroup >
                        <div class="control-group">
                        <apex:outputLabel styleClass="control-label" value="{!$Label.fieloee__selecttemplate}" />
                            <div class="controls">
                                <apex:selectList value="{!templateSelected}" size="1" >
                                    <apex:actionsupport event="onchange" action="{!changeTemplate}" />
                                    <apex:selectOptions value="{!templatesOptions}" ></apex:selectOptions>
                                </apex:selectList>
    <!--                             <span class="help-inline">{!$Label.InfoLayoutTitle}</span> -->
                            </div>
                        </div>
                    </apex:panelGroup>

                    <!-- Program -->
                    <apex:panelGroup >
                        <div class="control-group">
                        <apex:outputLabel styleClass="control-label" value="{!$ObjectType.FieloEE__ProgramLayout__c.fields.FieloEE__Program__c.Label}" />
                            <div class="controls">
                                <apex:outputField styleClass="input-xlarge" value="{!FieloEE__ProgramLayout__c.FieloEE__Program__c}" />                                
                            </div>
                        </div>
                    </apex:panelGroup>

                    <!-- Title -->
                    <apex:panelGroup >
                        <div class="control-group">
                        <apex:outputLabel styleClass="control-label" value="{!$ObjectType.FieloEE__ProgramLayout__c.fields.FieloEE__Title__c.Label}" />
                            <div class="controls">
                                <apex:inputText styleClass="input-xlarge" value="{!FieloEE__ProgramLayout__c.FieloEE__Title__c}" />
                                <span class="help-inline">{!$Label.fieloee__infolayouttitle}</span>
                            </div>
                        </div>
                    </apex:panelGroup>

                    <!-- Home -->
                    <apex:panelGroup >
                        <div class="control-group">
                        <apex:outputLabel styleClass="control-label" value="{!$ObjectType.FieloEE__ProgramLayout__c.fields.FieloEE__Home__c.Label}" />
                            <div class="controls">
                                <apex:inputField styleClass="input-xlarge" value="{!FieloEE__ProgramLayout__c.FieloEE__Home__c}" />
                            </div>
                        </div>
                    </apex:panelGroup>

                    <!-- Logo -->
                    <apex:panelGroup >
                        <div id="logo" class="control-group">
                            <apex:outputLabel styleClass="control-label" value="{!$Label.fieloee__logo}" />
                            <div class="controls" style="margin-top:5px;">
                                <div id="logoPreview" style="display:block;">
                                    <apex:panelGrid columns="2">
                                        <apex:image style="width:" rendered="{!!ISNULL(FieloEE__ProgramLayout__c.FieloEE__Logo__c)}" value="{!IF(LEFT(FieloEE__ProgramLayout__c.FieloEE__Logo__c,3) == '00P', URLFOR($Action.Attachment.Download, FieloEE__ProgramLayout__c.FieloEE__Logo__c), IF(LEFT(FieloEE__ProgramLayout__c.FieloEE__Logo__c,3) == '015', URLFOR($Action.Document.Download, FieloEE__ProgramLayout__c.FieloEE__Logo__c), URLFOR($Resource.FieloEE__Templates, FieloEE__ProgramLayout__c.FieloEE__Logo__c)))}" />
                                        <apex:panelGroup >
                                            <a href="javascript:void(0);" onclick="changeImage('logo');">
                                                <apex:outputText value="{!$Label.fieloee__changeimage}" rendered="{!!ISNULL(FieloEE__ProgramLayout__c.FieloEE__Logo__c)}" />
                                                <apex:outputText value="{!$Label.fieloee__uploadimage}" rendered="{!ISNULL(FieloEE__ProgramLayout__c.FieloEE__Logo__c)}" />
                                            </a><br/>
                                            <apex:commandLink value="{!$Label.fieloee__deleteimage}" action="{!deleteLogo}" onclick="if (!confirm('{!$Label.fieloee__areyousure}')) return false;" rendered="{!!ISNULL(FieloEE__ProgramLayout__c.FieloEE__Logo__c)}"/>
