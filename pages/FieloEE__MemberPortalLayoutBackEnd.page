        #categ-select, .categ   { padding: 4px 8px; }
        #comp-bar2 ul li    { float:left; list-style:none; padding: 4px 0; }
        #compbar-active { padding: 4px 6px 6px !important; }

        .reward     { border-radius:2px; border:1px solid #cccccc; margin:10px 0; height:160px; }
        #backPreview img    { width:100px; height:100px; }
        .advancedSettingsButton {display: block;color: #9C2222;font-weight: bold;}
    </style>
